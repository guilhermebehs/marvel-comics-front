ces.length > 0 ?\n                   Number(props.comic.prices[0].price).toFixed(2).replaceAll('.',',') \n                   :\n                   'O,00';\n    const title = props.comic.title || '';\n\n    return (\n         <>\n        {openDetails &&\n         <ComicDetails  comic={props.comic} close={()=>setOpenDetails(false)}/>\n         }\n        <div className=\"card\">\n            <img className=\"image\" onClick={()=>{setOpenDetails(true)}} src={imageUrl} alt=\"Imagem não disponível no momento.\"/>\n            <p onClick={()=>{setOpenDetails(true)}} className=\"title\">{title}</p>\n            <p>R$ {price}</p>\n            <input type=\"checkbox\" \n                checked={isChecked}\n                onChange={(e) => handleCheckBoxValue(e.target.checked)}/>\n        </div>\n        </>\n    )\n}\n\nexport default connector(ComicCArd);  ","/home/guilhermebehs/Documentos/estudo/marvel/marvel-comics-front/src/redux/reducers/comicReducer.ts",[],"/home/guilhermebehs/Documentos/estudo/marvel/marvel-comics-front/src/services/EmailService.ts",[],"/home/guilhermebehs/Documentos/estudo/marvel/marvel-comics-front/src/screens/ComicDetails.tsx",[],"/home/guilhermebehs/Documentos/estudo/marvel/marvel-comics-front/src/redux/types.ts",[],"/home/guilhermebehs/Documentos/estudo/marvel/marvel-comics-front/src/components/Search.tsx",[],"/home/guilhermebehs/Documentos/estudo/marvel/marvel-comics-front/src/screens/ConfirmSendEmail.tsx",[],{"ruleId":"74","replacedBy":"75"},{"ruleId":"76","replacedBy":"77"},{"ruleId":"78","severity":1,"message":"79","line":33,"column":7,"nodeType":"80","endLine":33,"endColumn":22,"suggestions":"81"},{"ruleId":"78","severity":1,"message":"79","line":39,"column":7,"nodeType":"80","endLine":39,"endColumn":40,"suggestions":"82"},{"ruleId":"78","severity":1,"message":"83","line":38,"column":7,"nodeType":"80","endLine":38,"endColumn":23,"suggestions":"84"},"no-native-reassign",["85"],"no-negated-in-lhs",["86"],"react-hooks/exhaustive-deps","React Ho
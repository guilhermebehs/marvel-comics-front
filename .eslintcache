owLoadingIcon, setShowLoadingIcon] = useState<boolean>(true);\n    const [offset, setOffset] = useState<number>(0);\n    const [limit, setLimit] = useState<number>(10);\n\n    function setListInitalState(){\n        setOffset(0);\n        setLimit(10);\n    }\n\n    useEffect(()=>{    \n        getData();\n    },[offset, limit])\n\n\n    useEffect(()=>{\n        setListInitalState();\n        getData();\n    },[props.refreshList, props.filter])\n\n   \n    async function getData(refreshParameters: boolean = false){\n        setShowLoadingIcon(true);\n        const comicService = new ComicsService();\n        const data:Array<IComic> = await comicService.getAll(limit, offset, props.filter);\n        const list = data.map((comic:IComic)=>{          \n            return (\n                <ComicCard comic={comic}/>\n            )\n          })\n          setShowLoadingIcon(false);\n          setComicList(list);\n     }\n\n     return (   \n        <>  \n        <LoadingIcon show={showLoadingIcon}/>   \n       \n            <div className=\"list\">\n            \n            {comicList}\n           \n            </div>\n            <div className=\"buttons\">\n              <button className=\"button-back\" disabled={offset <= 0} \n                  onClick={()=> setOffset(offset - limit)}> {\"<\"} </button>\n              <button className=\"button-next\" disabled={comicList.length < limit}\n                  onClick={()=> setOffset(offset + limit)}>{\">\"}</button>\n            </div>  \n           \n         </>\n    )\n}\n\nexport default connector(ComicList);  ","/home/guilhermebehs/Documentos/estudo/marvel/marvel-comics-front/src/components/TopBar.tsx",[],"/home/guilhermebehs/Documentos/estudo/marvel/marvel-comics-front/src/redux/reducers/rootReducer.ts",[],"/home/guilhermebehs/Documentos/estudo/marvel/marvel-comics-front/src/services/ComicsService.ts",[],"/home/guilhermebehs/Documentos/estudo/marvel/marvel-comics-front/src/components/LoadingIcon.tsx",[],"/home/gu